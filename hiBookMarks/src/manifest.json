{
    "manifest_version": 2,
    "name": "书签管理",
    "version": "0.0.10",
    "description": "在任何页面都可以通过书签关键字快速搜索打开书签,管理您的书签",
    "icons": {
        "128": "static/images/icon128.png"
    },
    "browser_action": {
        "default_icon": "static/images/icon128.png",
        "default_title": "书签管理",
        "default_popup": "popup.html"
    },
    "options_page": "index.html",
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+B",
                "mac": "Command+B",
                "chromeos": "Ctrl+B",
                "linux": "Ctrl+B"
            }
        }
    },
    "permissions": ["activeTab", "bookmarks", "tabs", "history", "<all_urls>"]
}