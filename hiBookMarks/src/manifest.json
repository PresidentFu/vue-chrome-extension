{
    "manifest_version": 2,
    "name": "书签管理",
    "version": "0.0.6",
    "description": "书签管理",
    "icons":
    {
        "128": "static/images/icon128.png"
    },
    "browser_action":
    {
        "default_icon": "static/images/icon128.png",
        "default_title": "书签管理",
        "default_popup": "popup.html"
    },
    "options_page": "index.html",
    "commands":
    {
        "_execute_browser_action":
        {
            "suggested_key":
            {
                "windows": "Ctrl+B",
                "mac": "Command+B",
                "chromeos": "Ctrl+B",
                "linux": "Ctrl+B"
            }
        }
    },
    "permissions": [
        "activeTab",
        "bookmarks",
        "tabs",
        "history",
        "<all_urls>"
    ],
    "homepageUrl": "https://chrome.google.com/webstore/detail/%E4%B9%A6%E7%AD%BE%E7%AE%A1%E7%90%86/kimcgbcdngdnggfmkjdmmjceijnkcjmd"
}