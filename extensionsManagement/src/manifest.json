{
    "manifest_version": 2,
    "name":"__MSG_appName__",
    "default_locale": "zh_CN",
    "version": "0.0.4",
    "description":"__MSG_appDescription__",
    "icons":
    {
        "128": "static/images/icon_128.png"
    },
    "browser_action":
    {
        "default_icon": "static/images/icon_128.png",
        "default_title": "扩展管理",
        "default_popup": "index.html"
    },
    "commands":
    {
        "_execute_browser_action":
        {
            "suggested_key":
            {
                "windows": "Ctrl+M",
                "mac": "Command+M",
                "chromeos": "Ctrl+M",
                "linux": "Ctrl+M"
            }
        }
    },
    "permissions": [
        "tabs",
        "management",
        "<all_urls>"
    ]
}